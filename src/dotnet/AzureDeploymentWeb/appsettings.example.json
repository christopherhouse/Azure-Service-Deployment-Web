    "ConnectionString": ""
  },
  "Cache": {
    "Redis": {
      "ConnectionString": ""
    },
    "SubscriptionsCacheDurationMinutes": 60,
    "ResourceGroupsCacheDurationMinutes": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}